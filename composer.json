{
    "name": "aberdeener/minecraft-oauth",
    "description": "Provides easy layer to get a Minecraft profile from a Microsoft Live OAuth session",
    "require": {
        "guzzlehttp/guzzle": "^7.5",
        "ramsey/uuid": "^3.9",
        "symfony/polyfill-php80": "^1.27"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Aberdeener\\MinecraftOauth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Tadhg Boyle",
            "email": "tadhgsmboyle@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "platform": {
            "php": "7.4"
        }
    }
}
